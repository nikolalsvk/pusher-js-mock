{
  "name": "pusher-js-mock",
  "version": "0.3.9",
  "description": "Mock Pusher.js in your JavaScript tests with ease",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config jest.config.js",
    "cover": "jest test --coverage",
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "docs": "typedoc",
    "precommit": "yarn lint && yarn format && git add -A src",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolalsvk/pusher-js-mock.git"
  },
  "keywords": [
    "pusher",
    "mock",
    "test"
  ],
  "author": "Nikola ƒêuza <nikolaseap@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nikolalsvk/pusher-js-mock/issues"
  },
  "homepage": "https://github.com/nikolalsvk/pusher-js-mock#readme",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@types/jest": "^24.0.24",
    "babel-jest": "^24.9.0",
    "husky": "^0.14.3",
    "jest": "^24.9.0",
    "jsdoc": "^3.5.5",
    "lint-staged": "^7.0.0",
    "prettier": "^1.19.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.5",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">=4.2.4"
  },
  "files": [
    "lib/**/*"
  ]
}
